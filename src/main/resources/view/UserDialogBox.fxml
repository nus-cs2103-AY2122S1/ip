<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-radius: 30;  -fx-background-color: LIGHTSTEELBLUE; -fx-background-insets: 5; -fx-padding: 15.0; -fx-alignment: top right; " type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" lineSpacing="1.5" maxHeight="1.7976931348623157E308" style="-fx-label-padding: 10.0;" text="Label" wrapText="true">
            <HBox.margin>
                <Insets />
            </HBox.margin>
            <font>
                <Font name="Arial" size="13.0" />
            </font></Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" style="-fx-scale-x: 0.8; -fx-scale-y: 0.8;">
            <HBox.margin>
                <Insets />
            </HBox.margin></ImageView>
    </children>
</fx:root>
