<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root
    alignment="TOP_RIGHT"
    minHeight="-Infinity"
    maxHeight="Infinity"
    maxWidth="1.7976931348623157E308"
    prefWidth="400.0"
    type="javafx.scene.layout.HBox"
    xmlns="http://javafx.com/javafx/11"
    xmlns:fx="http://javafx.com/fxml/1"
>
    <children>
        <Label
            fx:id="dialog"
            contentDisplay="TOP"
            maxHeight="Infinity"
            text="Label"
            wrapText="true"
        >
            <font>
                <Font
                    name="Monospaced Regular"
                    size="15.0"
                />
            </font>
         <HBox.margin>
            <Insets
                bottom="5.0"
                left="5.0"
                right="5.0"
                top="5.0"
            />
         </HBox.margin>
         <padding>
            <Insets
                bottom="5.0"
                left="5.0"
                right="5.0"
                top="5.0"
            />
         </padding>
      </Label>
      <Circle
        fx:id="displayPicture"
        fill="#1f93ff"
        radius="54.0"
        stroke="BLACK"
        strokeType="INSIDE"
      />
    </children>
    <padding>
        <Insets
            bottom="15.0"
            left="5.0"
            right="5.0"
            top="15.0"
        />
    </padding>
</fx:root>
