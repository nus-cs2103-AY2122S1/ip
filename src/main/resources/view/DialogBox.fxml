<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="300" maxWidth="1.7976931348623157E308" prefWidth="400.0" spacing="1.0" style="-fx-background-color: lightblue; -fx-border-width: 5; -fx-border-color: white; -fx-spacing: 1;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="CENTER" lineSpacing="1.0" text="Label" wrapText="true">
         <HBox.margin>
            <Insets right="5.0" />
         </HBox.margin>
         <font>
            <Font name="Times New Roman" size="13.0" />
         </font>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding></Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets />
         </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
