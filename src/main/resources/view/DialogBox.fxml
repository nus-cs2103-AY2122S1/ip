<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox  prefWidth="258.0" >
            <children>
                <Text fx:id="dialog"  text="Label" wrappingWidth="230" boundsType="LOGICAL">
                </Text>
            </children>
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
        </VBox>
        <ImageView fx:id="displayPicture" fitHeight="100" fitWidth="100" pickOnBounds="true" preserveRatio="true">
            <effect>
                <DropShadow />
            </effect>
         <HBox.margin>
            <Insets left="5.0" right="10.0" />
         </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
