<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         minHeight="-Infinity" prefWidth="400.0"
         style="-fx-padding: 0 10 10 10;"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0"/>
    </padding>
    <Label fx:id="dialog" minHeight="-Infinity"
           style="-fx-padding: 0 10 0 10;-fx-background-color: FFFFFF; -fx-background-radius: 10;" text="Label"
           wrapText="true">
        <HBox.margin>
            <Insets left="5.0" right="5.0"/>
        </HBox.margin>
        <font>
            <Font name="System Bold" size="12.5"/>
        </font>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true"/>
</fx:root>
