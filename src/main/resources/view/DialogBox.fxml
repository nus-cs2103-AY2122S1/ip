<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox maxHeight="-Infinity">
         <children>
              <Label fx:id="dialog" cache="true" maxWidth="-Infinity" minHeight="-Infinity" style="-fx-background-color: #008080; -fx-background-radius: 15px;" text="Label" textFill="WHITE" wrapText="true">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Label>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </HBox>
      <HBox maxHeight="100.0" maxWidth="100.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="15.0" right="15.0" />
         </HBox.margin>
         <children>
              <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin></ImageView>
         </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
