<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.text.Font?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="130.0" prefWidth="370.0" spacing="5.0" style="-fx-background-color: #2b5278; -fx-background-radius: 20px; -fx-border-radius: 20px;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <Text fx:id="dialog" fill="WHITE" text="Label" textAlignment="JUSTIFY">
            <HBox.margin>
                <Insets top="10.0" />
            </HBox.margin>
            <font>
                <Font name="Roboto" size="12.0" />
            </font>
        </Text>

        <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </ImageView>

    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
