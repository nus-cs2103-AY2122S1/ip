        <Label fx:id="dialog" text="Label" wrapText="true">
         <HBox.margin>
            <Insets left="8.0" right="8.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <font>
            <Font name="Open Sans Regular" size="14.0" />
         </font>
      </Label>
      <Circle fx:id="profilePicture" fill="DODGERBLUE" radius="32.0" stroke="TRANSPARENT" strokeType="INSIDE">
         <HBox.margin>
            <Insets left="8.0" right="8.0" />
         </HBox.margin></Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
