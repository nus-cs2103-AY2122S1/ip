<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<?import java.net.URL?>
<?import javafx.scene.text.Text?>
<fx:root alignment="TOP_RIGHT"
         maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="450.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
         styleClass="dialog-box-section">
    <children>
        <Text fx:id="dialog" text="Label" wrappingWidth = "250.0">
         <HBox.margin>
            <Insets left="2.0" right="2.0" />
         </HBox.margin>
        </Text>
<!--        <Label fx:id="dialog" text="Label" wrapText="true">-->
<!--         <HBox.margin>-->
<!--            <Insets left="2.0" right="2.0" />-->
<!--         </HBox.margin>-->
<!--        </Label>-->
        <ImageView fx:id="displayPicture" fitHeight="75.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="8.0" right="8.0" />
         </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
</fx:root>
