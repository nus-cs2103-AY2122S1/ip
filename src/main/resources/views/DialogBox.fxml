        <HBox fx:id="dialogContainer" opacity="0.8" styleClass="dialog-text-container">
            <Label fx:id="dialog" alignment="TOP_CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" text="Label" textFill="#0f0039" textOverrun="CLIP" wrapText="true">
