            </Label>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
        </HBox>
        <Circle fx:id="displayPicture" pickOnBounds="true" radius="27.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>

</fx:root>
