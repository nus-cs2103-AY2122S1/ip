<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="700.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="textMessage" text="Text" textFill="WHITE">
        <font>
            <Font name="Monaco" size="12.0" />
        </font>
        <HBox.margin>
            <Insets left="20.0" right="20.0" />
        </HBox.margin></Label>
    <padding>
        <Insets top="15.0" bottom="15.0" left = "10.0"/>
    </padding>
</fx:root>
